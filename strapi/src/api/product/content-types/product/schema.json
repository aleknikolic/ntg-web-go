{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "platform": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::platform.platform"
    },
    "category_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category-product.category-product"
    },
    "startPrice": {
      "type": "string"
    },
    "featuredProduct": {
      "type": "boolean",
      "default": false
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "contentTable": {
      "displayName": "contentTable",
      "type": "component",
      "repeatable": true,
      "component": "products.content-table"
    },
    "platformRating": {
      "displayName": "Product Rating",
      "type": "component",
      "repeatable": true,
      "component": "elements.product-rating"
    },
    "productMarketplace": {
      "displayName": "Product Marketplace",
      "type": "component",
      "repeatable": true,
      "component": "elements.product-marketplace"
    },
    "description": {
      "type": "richtext"
    },
    "customers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::customer.customer"
    },
    "service": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service.service"
    },
    "product_instance": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-instance.product-instance"
    },
    "summary": {
      "type": "richtext"
    },
    "contentSections": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "heros.hero",
        "layouts.accordion-content",
        "layouts.bkg-header",
        "layouts.card",
        "layouts.card-with-button",
        "layouts.center-hero",
        "layouts.component-type",
        "layouts.contact",
        "layouts.contact-info",
        "layouts.content-container",
        "layouts.featured-news",
        "layouts.header",
        "layouts.header-with-customer",
        "layouts.hero-circle-image",
        "layouts.hero-highlight",
        "layouts.hero-square-image",
        "layouts.image-hero",
        "layouts.item-hero",
        "layouts.mansory-image",
        "layouts.partner",
        "layouts.quote",
        "layouts.simple-hero",
        "layouts.step",
        "layouts.support-hero",
        "products.products"
      ]
    }
  }
}
